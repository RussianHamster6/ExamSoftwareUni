<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="624.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.UserView">
   <children>
      <TableView fx:id="UTable" layoutX="20.0" layoutY="64.0" onMouseClicked="#editUser" prefHeight="272.0" prefWidth="470.0">
        <columns>
          <TableColumn fx:id="UIDCol" prefWidth="126.0" text="UserID" />
          <TableColumn fx:id="FNameCol" prefWidth="134.0" text="First Name" />
            <TableColumn fx:id="SNameCol" minWidth="3.0" prefWidth="130.0" text="Surname" />
            <TableColumn fx:id="IsEmpCol" prefWidth="79.0" text="isEmployee" />
        </columns>
      </TableView>
      <Button layoutX="513.0" layoutY="64.0" mnemonicParsing="false" onAction="#addUser" prefHeight="56.0" prefWidth="97.0" text="Add" />
   </children>
</AnchorPane>
